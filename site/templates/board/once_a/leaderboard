<table cellspacing=0 cellpadding=0 class=chain>
  <thead>
    <tr>
      <th colspan=3>
       [% IF next %]<a href="[% next %].html">«</a> [% ELSIF previous %]« [% END %]
       [% url ? "<a href=\"$url\">$title</a>" : "$title" %]
       [% IF previous %] <a href="[% previous %].html">»</a>[% ELSIF next %] »[%END -%]
      </th>
    </tr>
  </thead>
  <tbody>
  [% WHILE ( entry = entries.next ) %][% LAST IF limit.defined && entry.rank > limit %]
    <tr class="author[% IF entry.safe %] safe[% ELSIF entry.active %] active[% ELSIF entry.fallen %] fallen[% END %]">
      <td align=left>[% entry.rank == prev ? '-&nbsp;' : entry.rank _ '.' %]</td>
      <td align=left><a href="https://metacpan.org/author/[% entry.author %]">[% entry.author %]</a></td>
      <td align=right>[% entry.count %]</td>
    </tr>
  [% prev = entry.rank ; END %]
  </tbody>
</table>
